                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT
        a.*, 
		--concat(CAST(CERTIFICATE_IDENTIFIER AS VARCHAR(12)), '*', CAST(NULL AS VARCHAR(12)), '*', CAST(NULL AS VARCHAR(12))) as compound_key,
		max(delinquency_id) over(partition by  CERTIFICATE_IDENTIFIER) as MAX_DEL_ID --into HLP_LES_DELINQUENCY_TMP
FROM (
        select 
                dlc.DELINQUENCY_ID,
                lc.CONTRACT_IDENTIFIER,
                lc.INSURED_LOAN_IDENTIFIER,
                NULL as CERTIFICATE_IDENTIFIER,
                cv1.SHORT_NAME as STATUS_CODE,
                cv2.SHORT_NAME as DELINQUENCY_STAGE_CODE,
                cv3.SHORT_NAME as DEL_STATUS_CHANGE_REASON_CODE,
				ISNULL(lc.LINE_OF_BUSINESS,0) as LINE_OF_BUSINESS
        from    dbo.STG_LOAN_COVERAGE lc,
                dbo.STG_DELINQUENCY_LOAN_COVERAGE dlc
                left outer join dbo.STG_CODE_VALUE cv1
                on dlc.STATUS_CODE = cv1.CODE_VALUE_ID
                left outer join dbo.STG_CODE_VALUE cv2
                on dlc.DELINQUENCY_STAGE_CODE = cv2.CODE_VALUE_ID
                left outer join dbo.STG_CODE_VALUE cv3
                on dlc.STATUS_CHANGE_REASON_CODE = cv3.CODE_VALUE_ID,
                edw_dm.dbo.LKP_TIME_ZONE_CONVERSION LKP
        where  lc.LOAN_COVERAGE_ID = dlc.LOAN_COVERAGE_ID
        and    lc.CONTRACT_IDENTIFIER is not NULL
        and    lc.INSURED_LOAN_IDENTIFIER is not NULL
        and    dlc.audit_timestamp &gt;=time_offset_start_date
        and    dlc.audit_timestamp &lt; time_offset_end_date
        and    dateadd(hh,time_offset,dlc.audit_timestamp) = ( select max(dateadd(hh,time_offset,dlci.audit_timestamp))
                                                                from   dbo.STG_LOAN_COVERAGE lci,
                                                                    dbo.STG_DELINQUENCY_LOAN_COVERAGE dlci,
                                                                    edw_dm.dbo.LKP_TIME_ZONE_CONVERSION LKPi
                                                                    where  lci.LOAN_COVERAGE_ID = dlci.LOAN_COVERAGE_ID
                                                                    and    lci.CONTRACT_IDENTIFIER is not NULL
                                                                    and       lci.INSURED_LOAN_IDENTIFIER is not NULL
                                                                    and    dlci.audit_timestamp &gt;=LKPi.time_offset_start_date
                                                                    and    dlci.audit_timestamp &lt; LKPi.time_offset_end_date
                                                                    and       dateadd(hh,time_offset,dlci.audit_timestamp) &lt; cast(ltrim(rtrim(cast(year(getdate()) as char))) +ltrim(rtrim('-'))+
                                                                    ltrim(rtrim(cast(month(getdate()) as char)))+ltrim(rtrim('-'))+ltrim(rtrim('01'))
                                                                    as datetime)
                                                                    and    lc.LOAN_COVERAGE_ID =lci.LOAN_COVERAGE_ID
                                                                    group by lci.LOAN_COVERAGE_ID)
       
        UNION 

        select dlc.DELINQUENCY_ID,
                    NULL as CONTRACT_IDENTIFIER,
                    NULL as INSURED_LOAN_IDENTIFIER,
                    lc.CERTIFICATE_IDENTIFIER,
                    cv1.SHORT_NAME as STATUS_CODE,
                    cv2.SHORT_NAME as DELINQUENCY_STAGE_CODE,
                    cv3.SHORT_NAME as DEL_STATUS_CHANGE_REASON_CODE,
					ISNULL(lc.LINE_OF_BUSINESS,0) as LINE_OF_BUSINESS
        from   
                dbo.STG_LOAN_COVERAGE lc,
                dbo.STG_DELINQUENCY_LOAN_COVERAGE dlc
                left outer join dbo.STG_CODE_VALUE cv1
                on dlc.STATUS_CODE = cv1.CODE_VALUE_ID
                left outer join dbo.STG_CODE_VALUE cv2
                on dlc.DELINQUENCY_STAGE_CODE = cv2.CODE_VALUE_ID
                left outer join dbo.STG_CODE_VALUE cv3
                on dlc.STATUS_CHANGE_REASON_CODE = cv3.CODE_VALUE_ID,
                edw_dm.dbo.LKP_TIME_ZONE_CONVERSION LKP
        where  lc.LOAN_COVERAGE_ID = dlc.LOAN_COVERAGE_ID
        and    lc.CERTIFICATE_IDENTIFIER is not NULL
        and    dlc.audit_timestamp &gt;=time_offset_start_date
        and    dlc.audit_timestamp &lt; time_offset_end_date
        and    dateadd(hh,time_offset,dlc.audit_timestamp) = ( select max(dateadd(hh,time_offset,dlci.audit_timestamp))
                                                                from   dbo.STG_LOAN_COVERAGE lci,
                                                                    dbo.STG_DELINQUENCY_LOAN_COVERAGE dlci,
                                                                    edw_dm.dbo.LKP_TIME_ZONE_CONVERSION LKPi
                                                                    where  lci.LOAN_COVERAGE_ID = dlci.LOAN_COVERAGE_ID
                                                                    and       lci.CERTIFICATE_IDENTIFIER is not NULL
                                                                    and    dlci.audit_timestamp &gt;=LKPi.time_offset_start_date
                                                                    and    dlci.audit_timestamp &lt; LKPi.time_offset_end_date
                                                                    and       dateadd(hh,time_offset,dlci.audit_timestamp) &lt; cast(ltrim(rtrim(cast(year(getdate()) as char))) +ltrim(rtrim('-'))+
                                                                    ltrim(rtrim(cast(month(getdate()) as char)))+ltrim(rtrim('-'))+ltrim(rtrim('01'))
                                                                    as datetime)
                                                                    and    lc.LOAN_COVERAGE_ID =lci.LOAN_COVERAGE_ID
                                                                    group by lci.LOAN_COVERAGE_ID)
) a;</property>
