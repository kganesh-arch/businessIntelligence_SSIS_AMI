    <DTS:Executable
      DTS:refId="Package\HLP_FCT_LOAN_EVOLUTION_STATUS"
      DTS:CreationName="Microsoft.Pipeline"
      DTS:DelayValidation="True"
      DTS:Description="Data Flow Task"
      DTS:DTSID="{B2E21E97-AE27-4354-B931-72FF1B2F1286}"
      DTS:ExecutableType="Microsoft.Pipeline"
      DTS:LocaleID="-1"
      DTS:ObjectName="HLP_FCT_LOAN_EVOLUTION_STATUS"
      DTS:TaskContact="Performs high-performance data extraction, transformation and loading;Microsoft Corporation; Microsoft SQL Server; (C) Microsoft Corporation; All Rights Reserved;http://www.microsoft.com/sql/support/default.asp;1">
      <DTS:Variables />
      <DTS:ObjectData>
        <pipeline
          version="1">
          <components>
            <component
              refId="Package\HLP_FCT_LOAN_EVOLUTION_STATUS\src_PARTITION_STAGING_FCT_LOAN_EVOLUTION_STATUS"
              componentClassID="Microsoft.OLEDBSource"
              contactInfo="OLE DB Source;Microsoft Corporation; Microsoft SQL Server; (C) Microsoft Corporation; All Rights Reserved; http://www.microsoft.com/sql/support;7"
              description="OLE DB Source"
              name="src_PARTITION_STAGING_FCT_LOAN_EVOLUTION_STATUS"
              usesDispositions="true"
              validateExternalMetadata="False"
              version="7">
              <properties>
                <property
                  dataType="System.Int32"
                  description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                  name="CommandTimeout">0</property>
                <property
                  dataType="System.String"
                  description="Specifies the name of the database object used to open a rowset."
                  name="OpenRowset"></property>
                <property
                  dataType="System.String"
                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">DECLARE @Cntrl_Dt int

SET @Cntrl_Dt = ? --BI-1897 put back to ?

BEGIN

WITH src_HLP_FCT_LOAN_EVOL_STATUS_BK_BASE AS 
(
       select x.*, 
       MAX(FILED_DATE) over(partition by COVERAGE_BK) as MAX_FILED_DT, 
       MAX(AUDIT_TIMESTAMP) over(partition by COVERAGE_BK, FILED_DATE) as MAX_AUDIT_TS,
       MAX(BANKRUPTCY_ID) over(partition by COVERAGE_BK, FILED_DATE, AUDIT_TIMESTAMP) as MAX_BANK_ID
       from
       (
              select 
                     lc.LOAN_COVERAGE_ID,
                     lc.CONTRACT_IDENTIFIER,
                     lc.INSURED_LOAN_IDENTIFIER,
                     lc.CERTIFICATE_IDENTIFIER,
                     b.BANKRUPTCY_ID,
                     cv1.SHORT_NAME as CHAPTER_CODE,
                     cv2.SHORT_NAME as STATUS_CODE,
                     cast( convert( varchar, b.FILED_DATE, 112 ) as int ) as FILED_DATE,
                     cast( convert( varchar, b.DISCHARGE_DATE, 112 ) as int ) as DISCHARGE_DATE,
                     case
                           when cast( convert( varchar, b.DISCHARGE_DATE, 112 ) as int ) &gt; @Cntrl_Dt or b.DISCHARGE_DATE is NULL
                           then 'Current Bankrupt'
                           else
                                  case
                                         when cast( convert( varchar, b.DISCHARGE_DATE, 112 ) as int ) &lt;= @Cntrl_Dt
                                         then 'Past Bankrupt'
                                         else 'Never Bankrupt'
                                  end
                     end as BANKRUPTSY_STAGE,
                     case
                           when cast( convert( varchar, b.DISCHARGE_DATE, 112 ) as int ) &gt; @Cntrl_Dt or b.DISCHARGE_DATE is NULL
                           then 2
                           else
                                  case
                                         when cast( convert( varchar, b.DISCHARGE_DATE, 112 ) as int ) &lt;= @Cntrl_Dt
                                         then 1
                                         else 0
                                  end
                     end as BANKRUPTSY_STAGE_SCORE,
                     concat(CAST(lc.CERTIFICATE_IDENTIFIER AS VARCHAR(12)), '*', CAST(lc.CONTRACT_IDENTIFIER AS VARCHAR(12)), '*', CAST(lc.INSURED_LOAN_IDENTIFIER AS VARCHAR(12))) as COVERAGE_BK,
                     convert( varchar(26), b.AUDIT_TIMESTAMP, 109) as AUDIT_TIMESTAMP,
					 ISNULL(lc.LINE_OF_BUSINESS,0) as LINE_OF_BUSINESS
              from   
                     dbo.STG_BANKRUPTCY b,
                     dbo.STG_BANKRUPTCY_LOAN_COVERAGE blc,
                     dbo.STG_LOAN_COVERAGE lc,
                     dbo.STG_CODE_VALUE cv1,
                     dbo.STG_CODE_VALUE cv2
              where  
              b.BANKRUPTCY_ID = blc.BANKRUPTCY_ID
              and    blc.LOAN_COVERAGE_ID = lc.LOAN_COVERAGE_ID
              and    b.CHAPTER_CODE = cv1.CODE_VALUE_ID
              and    b.STATUS_CODE = cv2.CODE_VALUE_ID
              and    b.FILED_DATE is not NULL
              and    b.CHAPTER_CODE is not NULL
              and    cv2.SHORT_NAME = 'A'
              and    b.FILED_DATE = (
                     select max( b1.FILED_DATE )
                     from   dbo.STG_BANKRUPTCY b1
                     where  b1.BANKRUPTCY_ID = b.BANKRUPTCY_ID )

              union

              select 
                     lc.LOAN_COVERAGE_ID,
                     lc.CONTRACT_IDENTIFIER,
                     lc.INSURED_LOAN_IDENTIFIER,
                     lc.CERTIFICATE_IDENTIFIER,
                     b.BANKRUPTCY_ID,
                     cv1.SHORT_NAME as CHAPTER_CODE,
                     cv2.SHORT_NAME as STATUS_CODE,
                     cast( convert( varchar, b.FILED_DATE, 112 ) as int ) as FILED_DATE,
                     cast( convert( varchar, b.DISCHARGE_DATE, 112 ) as int ) as DISCHARGE_DATE,
                     case
                           when cast( convert( varchar, b.DISCHARGE_DATE, 112 ) as int ) &gt; @Cntrl_Dt or b.DISCHARGE_DATE is NULL
                           then 'Current Bankrupt'
                           else
                                  case
                                         when cast( convert( varchar, b.DISCHARGE_DATE, 112 ) as int ) &lt;= @Cntrl_Dt
                                         then 'Past Bankrupt'
                                         else 'Never Bankrupt'
                                  end
                     end as BANKRUPTSY_STAGE,
                     case
                           when cast( convert( varchar, b.DISCHARGE_DATE, 112 ) as int ) &gt; @Cntrl_Dt or b.DISCHARGE_DATE is NULL
                           then 2
                           else
                                  case
                                         when cast( convert( varchar, b.DISCHARGE_DATE, 112 ) as int ) &lt;= @Cntrl_Dt
                                         then 1
                                         else 0
                                  end
                     end as BANKRUPTSY_STAGE_SCORE,
                     concat(CAST(lc.CERTIFICATE_IDENTIFIER AS VARCHAR(12)), '*', CAST(lc.CONTRACT_IDENTIFIER AS VARCHAR(12)), '*', CAST(lc.INSURED_LOAN_IDENTIFIER AS VARCHAR(12))) as COVERAGE_BK,
                     convert( varchar(26), b.AUDIT_TIMESTAMP, 109) as AUDIT_TIMESTAMP,
					 ISNULL(lc.LINE_OF_BUSINESS,0) as LINE_OF_BUSINESS
              from   
                     dbo.STG_BANKRUPTCY_AUDIT b,
                     dbo.STG_BANKRUPTCY_LOAN_COVERAGE blc,
                     dbo.STG_LOAN_COVERAGE lc,
                     dbo.STG_CODE_VALUE cv1,
                     dbo.STG_CODE_VALUE cv2
              where  
                     b.BANKRUPTCY_ID = blc.BANKRUPTCY_ID
                     and    b.BPA_AUDIT_TRAIL_ID = blc.BPA_AUDIT_TRAIL_ID
                     and    blc.LOAN_COVERAGE_ID = lc.LOAN_COVERAGE_ID
                     and    blc.BPA_AUDIT_TRAIL_ID = lc.BPA_AUDIT_TRAIL_ID
                     and    b.CHAPTER_CODE = cv1.CODE_VALUE_ID
                     and    b.STATUS_CODE = cv2.CODE_VALUE_ID
                     and    b.FILED_DATE is not NULL
                     and    b.CHAPTER_CODE is not NULL
                     and    cv2.SHORT_NAME = 'A'
                     and    b.FILED_DATE = (
                           select max( b1.FILED_DATE )
                           from   dbo.STG_BANKRUPTCY b1
                           where  b1.BANKRUPTCY_ID = b.BANKRUPTCY_ID )
       ) x
)
select distinct z.*,
CASE SCORE 
              WHEN 12 THEN 'PD'
              WHEN 11 THEN 'RC'
              WHEN 10 THEN 'RNC'
              WHEN 9 THEN 'TCI'
              WHEN 8 THEN 'TNCNR'
              WHEN 7 THEN 'F'
              WHEN 6 THEN 'A'
              WHEN 5 THEN 'B'
              WHEN 4 THEN 'C'
              WHEN 3 THEN 'D'
              WHEN 2 THEN 'E'
              WHEN 1 THEN 'AC'
              WHEN 0 THEN 'Active'
       END as Loan_Evolution_Status_Cd,
       CASE score
              WHEN 12 THEN 'Paid'
              WHEN 11 THEN 'Rescinded (Claim)'
              WHEN 10 THEN 'Rescinded (Non-Claim)'
              WHEN 9 THEN 'Terminated (Coverage Ineligible)'
              WHEN 8 THEN 'Terminated (Non-Claim/Rescission)'
              WHEN 7 THEN 'Claim Filed'
              WHEN 6 THEN 'Less than 4 months DQ'
              WHEN 5 THEN 'Greater than 4 months DQ'
              WHEN 4 THEN 'Foreclosure Begins'
              WHEN 3 THEN 'Foreclosure Date Set'
              WHEN 2 THEN 'Lender Has Title'
              WHEN 1 THEN 'Active - Cured'
              WHEN 0 THEN 'Active'
       END as Loan_Evolution_Status_Desc
from
(
       select 
			 case when lesb.line_of_business = 0 OR lesb.CERTIFICATE_NBR_BK_UG is null then --BI-1897
				-- West
				case when dm_cert.CERTIFICATE_SK is not null then dm_cert.CERTIFICATE_SK 
					else (select CERTIFICATE_SK from edw_dm.dbo.DIM_MI_Certificate where CERTIFICATE_NBR_BK = '-1')
				end
				else
				-- East
				case when dm_cert_east.CERTIFICATE_SK is not null then dm_cert_east.CERTIFICATE_SK
					else (select CERTIFICATE_SK from edw_dm.dbo.DIM_MI_Certificate where CERTIFICATE_NBR_BK = '-1')
				end
              end as certificate_sk,
              @Cntrl_Dt AS fiscal_period_sid,
              
			  case when lesb.line_of_business = 0 OR lesb.CERTIFICATE_NBR_BK_UG is null then --BI-1897
			    -- West
				case when dm_ln.LOAN_SK is not null then dm_ln.LOAN_SK 
                     else (select LOAN_SK from edw_dm.dbo.DIM_LOAN where CERTIFICATE_NBR_BK = '-1')
				end
				else
				-- East
				case when dm_ln_east.LOAN_SK is not null then dm_ln_east.LOAN_SK 
                     else (select LOAN_SK from edw_dm.dbo.DIM_LOAN where CERTIFICATE_NBR_BK = '-1')
				end
              end as LOAN_SK,

              case when dm_cov.loan_coverage_sid is not null
                           then dm_cov.loan_coverage_sid 
                     else (select loan_coverage_sid from edw_dm.dbo.DIM_LOAN_COVERAGE where loan_coverage_nbr_bk = 'Invalid')
              end as loan_coverage_sid,
              case when dm_mort.mortgage_loan_sid is not null
                           then dm_mort.mortgage_loan_sid 
                     else (select mortgage_loan_sid from edw_dm.dbo.DIM_MORTGAGE_LOAN where mortgage_loan_id_bk = 'Invalid')
              end as mortgage_loan_sid,
              case when dm_prd.product_sid is not null
                           then dm_prd.product_sid 
                     else (select product_sid from edw_dm.dbo.DIM_PRODUCT where product_cd_bk = 'Invalid')
              end as product_sid,
              case when dm_con.contract_sid is not null
                           then dm_con.contract_sid 
                     else (select contract_sid from edw_dm.dbo.DIM_CONTRACT where contract_policy_nbr_bk = 'Invalid')
              end as contract_sid,
              
			  ISNULL(b.FILED_DATE,-1) as valid_bankruptcy_filed_dt_sk,
              (CASE WHEN lesc.STATUS_CODE = 'PCLM' and lesc.TYPE_CODE = 'CLMI' AND ISNULL( lesc.STATUS_CHANGE_REASON_CODE, '' ) &lt;&gt; 'RES'
                           THEN 12
                     ELSE 
                           CASE WHEN --((lesb.COVERAGE_TYPE = 'SECONDARY' and lesb.LOAN_COVERAGE_STATUS_CODE = 'T')or
                                  /*lesb.COVERAGE_TYPE = 'PRIMARY' and*/ lesb.STATUS_CODE = 'T'
                                  and lesc.STATUS_CODE = 'DLCLM' 
                                  and lesc.TYPE_CODE = 'CLMI' 
                                  and ISNULL( lesc.STATUS_CHANGE_REASON_CODE, '' ) in ('RES','DLTRSN9')
                                  THEN 11
                           ELSE
                                  CASE WHEN --( lesb.COVERAGE_TYPE = 'SECONDARY' and lesb.LOAN_COVERAGE_STATUS_CODE = 'T' and lesb.CVRG_TERMINATION_REASON_CODE = 'RS' ) or
                                         /*lesb.COVERAGE_TYPE = 'PRIMARY' and*/ lesb.STATUS_CODE = 'T' and lesb.TERMINATION_TYPE_CODE = 'RS'
                                         THEN 10
                                  ELSE
                                         CASE WHEN --( lesb.COVERAGE_TYPE = 'SECONDARY' and lesb.LOAN_COVERAGE_STATUS_CODE = 'T' and lesb.CVRG_TERMINATION_REASON_CODE = 'IN' ) or
                                                /*lesb.COVERAGE_TYPE = 'PRIMARY' and*/ lesb.STATUS_CODE = 'T' and lesb.TERMINATION_TYPE_CODE = 'IN'
                                                THEN 9
                                         ELSE
                                                CASE WHEN --( ISNULL(lesc.STATUS_CODE,'') &lt;&gt; 'PCLM') and (( lesb.COVERAGE_TYPE = 'SECONDARY' and lesb.lesb.STATUS_CODE = 'T' and ISNULL(lesb.CVRG_TERMINATION_REASON_CODE,'') NOT IN ('RS','IN') ) or
                                                       /*lesb.COVERAGE_TYPE = 'PRIMARY' and*/ lesb.STATUS_CODE = 'T' and ISNULL(lesb.TERMINATION_TYPE_CODE,'') NOT IN ('RS','IN')
                                                       THEN 8
                                                ELSE
                                                       CASE WHEN CAST( ISNULL( lesc.CLAIM_ID, 0 ) AS INTEGER) &lt;&gt; CAST(0 AS INTEGER) and ISNULL(lesc.STATUS_CODE,'') not in('PCLM','DLCLM')
                                                              THEN 7
                                                       ELSE
                                                              CASE WHEN lesd.STATUS_CODE = 'A' and lesd.DELINQUENCY_STAGE_CODE = 'A'
                                                                     THEN 6
                                                              ELSE
                                                                     CASE WHEN lesd.STATUS_CODE = 'A' and lesd.DELINQUENCY_STAGE_CODE = 'B'
                                                                                  THEN 5
                                                                           WHEN lesd.STATUS_CODE = 'A' and lesd.DELINQUENCY_STAGE_CODE = 'C'
                                                                                  THEN 4
                                                                           WHEN lesd.STATUS_CODE = 'A' and lesd.DELINQUENCY_STAGE_CODE = 'D'
                                                                                  THEN 3
                                                                            WHEN lesd.STATUS_CODE = 'A' and lesd.DELINQUENCY_STAGE_CODE = 'E'
                                                                                  THEN 2
                                                                     ELSE
                                                                           CASE WHEN ISNULL(lesd.STATUS_CODE,'ZZZ') in ('C','D') and ISNULL(lesd.DEL_STATUS_CHANGE_REASON_CODE,'ZZZ') not in ('CP','CPDM')
                                                                                  THEN 1
                                                                           ELSE 0
                                                                           END
                                                                     END
                                                              END
                                                       END
                                                END 
                                         END 
                                  END
                           END
                     END) score,
              b.CHAPTER_CODE as valid_bankruptcy_chapter_cd,
              ISNULL(b.DISCHARGE_DATE,-1) as valid_bankruptcy_discharge_dt_sk,
              ISNULL(b.BANKRUPTSY_STAGE, 'Never Bankrupt' ) as bankruptcy_stage,
              c.Score as early_indicator_score 
		FROM    
		(	-- West, East not in UGC400
			SELECT 
				CTIDCM * 10 + CTCKCM as CERTIFICATE_IDENTIFIER,
				STS@CM AS STATUS_CODE,
				TTP@CM AS TERMINATION_TYPE_CODE,
				--0 AS LINE_OF_BUSINESS --BI-1897
				LOB#CM AS LINE_OF_BUSINESS,
				NULL CERTIFICATE_NBR_BK_UG --BI-1897
			FROM 
				EDW_STAGING.dbo.STG_ISPFPCM 
			UNION
			-- Legacy East
			SELECT 
				CERTIFICATE_NBR_BK,
				CURRENT_CERTIFICATE_STATUS_CD,
				CERT_TERMINATION_TYPE_CD,
				INSURANCE_LINE_CODE,
				CERTIFICATE_NBR_BK CERTIFICATE_NBR_BK_UG --BI-1897
			FROM 
				EDW_STAGING.dbo.STG_FCT_DAILY_VOLUME_UG
		) lesb
          			  
		LEFT OUTER JOIN dbo.HLP_LES_DELINQUENCY_TMP lesd
        -- on CTIDCM * 10 + CTCKCM = lesd.CERTIFICATE_IDENTIFIER 
		on lesb.CERTIFICATE_IDENTIFIER = lesd.CERTIFICATE_IDENTIFIER 
		and lesb.LINE_OF_BUSINESS = lesd.LINE_OF_BUSINESS
		and lesd.delinquency_id = lesd.MAX_DEL_ID

        LEFT OUTER JOIN dbo.HLP_LES_CLAIM_TMP lesc
        --on CTIDCM * 10 + CTCKCM = lesc.CERTIFICATE_IDENTIFIER
		on lesb.CERTIFICATE_IDENTIFIER = lesc.CERTIFICATE_IDENTIFIER
		and lesb.LINE_OF_BUSINESS = lesc.LINE_OF_BUSINESS

        LEFT OUTER JOIN
        (
                SELECT distinct 
                    b1.CONTRACT_IDENTIFIER,
                    b1.INSURED_LOAN_IDENTIFIER,
                    b1.CERTIFICATE_IDENTIFIER,
                    b1.CHAPTER_CODE,
                    b1.FILED_DATE,
                    b1.DISCHARGE_DATE,
                    b1.BANKRUPTSY_STAGE,
                    b1.coverage_bk,
                    b1.BANKRUPTCY_ID,
                    b1.AUDIT_TIMESTAMP,
					b1.LINE_OF_BUSINESS
                FROM   
                    src_HLP_FCT_LOAN_EVOL_STATUS_BK_BASE b1
                WHERE  
                    b1.FILED_DATE = b1.MAX_FILED_DT
                    and b1.AUDIT_TIMESTAMP = b1.MAX_AUDIT_TS
                    and b1.BANKRUPTCY_ID = b1.MAX_BANK_ID
        ) b
        -- on CTIDCM * 10 + CTCKCM = b.CERTIFICATE_IDENTIFIER 
		on lesb.CERTIFICATE_IDENTIFIER = b.CERTIFICATE_IDENTIFIER
			and lesb.LINE_OF_BUSINESS = b.LINE_OF_BUSINESS
			and b.CONTRACT_IDENTIFIER IS NULL 
			and b.INSURED_LOAN_IDENTIFIER IS NULL
        
		LEFT OUTER JOIN
        (
                SELECT 
                    CTIDWS,
                    score,
                    TapeDate,
					ISNULL(Line_of_business,0) as Line_of_business
                FROM   
                    dbo.STG_EARLY_INDICATOR_SCORE a left join DBO.STG_LOAN_COVERAGE b
				on  
					a.CTIDWS = b.CERTIFICATE_IDENTIFIER
                where 
                    fatalerror = 'N'
                    and CAST(TapeDate AS INT) = @Cntrl_Dt
        ) c
        --on CTIDCM * 10 + CTCKCM = c.CTIDWS
		on lesb.CERTIFICATE_IDENTIFIER = c.CTIDWS
		and lesb.LINE_OF_BUSINESS = c.LINE_OF_BUSINESS
        
		LEFT OUTER JOIN EDW_DM.dbo.DIM_MI_Certificate dm_cert
        --ON CTIDCM * 10 + CTCKCM = dm_cert.CERTIFICATE_NBR_BK
		ON lesb.CERTIFICATE_IDENTIFIER = dm_cert.CERTIFICATE_NBR_BK_AMIWEST AND dm_cert.curr_ind = 'Y'
        
		LEFT OUTER JOIN EDW_DM.dbo.DIM_MI_Certificate dm_cert_east
		ON lesb.CERTIFICATE_IDENTIFIER = dm_cert_east.CERTIFICATE_NBR_BK_UG AND dm_cert_east.curr_ind = 'Y'

		LEFT OUTER JOIN EDW_DM.dbo.DIM_LOAN_COVERAGE dm_cov
        ON dm_cov.loan_coverage_nbr_bk = 'Invalid' AND dm_cov.curr_ind = 'Y'
        LEFT OUTER JOIN EDW_DM.dbo.DIM_MORTGAGE_LOAN dm_mort
        ON dm_mort.mortgage_loan_id_bk = 'Invalid' AND dm_mort.curr_ind = 'Y'
        LEFT OUTER JOIN EDW_DM.dbo.DIM_CONTRACT dm_con
        ON dm_con.contract_policy_nbr_bk = 'Invalid'AND dm_con.curr_ind = 'Y'
        LEFT OUTER JOIN EDW_DM.dbo.DIM_PRODUCT dm_prd
        ON dm_prd.product_cd_bk = 'Invalid' AND dm_prd.curr_ind = 'Y'
        
		LEFT OUTER JOIN EDW_DM.dbo.DIM_LOAN dm_ln
        --ON CTIDCM * 10 + CTCKCM = dm_ln.CERTIFICATE_NBR_BK 
		ON lesb.CERTIFICATE_IDENTIFIER = dm_ln.CERTIFICATE_NBR_BK_AMIWEST AND dm_ln.curr_ind = 'Y'

		LEFT OUTER JOIN EDW_DM.dbo.DIM_LOAN dm_ln_east
		ON lesb.CERTIFICATE_IDENTIFIER = dm_ln_east.CERTIFICATE_NBR_BK_UG AND dm_ln_east.curr_ind = 'Y'

) z

END</property>
