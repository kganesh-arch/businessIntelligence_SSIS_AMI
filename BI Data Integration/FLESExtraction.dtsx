                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">--TRUNCATE TABLE HLP_LES_CLAIM_TMP

SELECT a.* --INTO dbo.HLP_LES_CLAIM_TMP
	FROM (
	select 
		--concat(CAST(CERTIFICATE_IDENTIFIER AS VARCHAR(12)), '*', CAST(NULL AS VARCHAR(12)), '*', CAST(NULL AS VARCHAR(12))) as compound_key,
		c.CLAIM_ID,
			NULL as CONTRACT_IDENTIFIER,
			NULL as INSURED_LOAN_IDENTIFIER,
			CERTIFICATE_IDENTIFIER,
			cv1.SHORT_NAME as STATUS_CODE,
			cv2.SHORT_NAME as TYPE_CODE,
			cv3.SHORT_NAME as STATUS_CHANGE_REASON_CODE,
			ISNULL(C.LINE_OF_BUSINESS,0) AS LINE_OF_BUSINESS -- Modified 20170118; BI-1610
	from   dbo.STG_CLAIM c
			left outer join dbo.STG_CLAIM_CODE_VALUE cv1
				on c.STATUS_CODE = cv1.CODE_VALUE_ID
			left outer join dbo.STG_CLAIM_CODE_VALUE cv2
				on c.TYPE_CODE = cv2.CODE_VALUE_ID
			left outer join dbo.STG_CLAIM_CODE_VALUE cv3
				on STATUS_CHANGE_REASON_CODE = cv3.CODE_VALUE_ID,
				edw_dm.dbo.LKP_TIME_ZONE_CONVERSION LKP
	where  CERTIFICATE_IDENTIFIER is not NULL
	and           cv2.SHORT_NAME = 'CLMI'
	and    c.audit_timestamp &gt;=time_offset_start_date
	and    c.audit_timestamp &lt; time_offset_end_date
	and    dateadd(hh,time_offset,c.audit_timestamp) = ( select max(dateadd(hh,time_offset,ci.audit_timestamp))
															from   dbo.STG_CLAIM ci,
																		edw_dm.dbo.LKP_TIME_ZONE_CONVERSION LKPi,
																		dbo.STG_CLAIM_CODE_VALUE cvi
																where       ci.CERTIFICATE_IDENTIFIER is not NULL
																and           ci.TYPE_CODE = cvi.CODE_VALUE_ID
																and           cvi.SHORT_NAME = 'CLMI'
																and    ci.audit_timestamp &gt;=LKPi.time_offset_start_date
																and    ci.audit_timestamp &lt; LKPi.time_offset_end_date
																and       dateadd(hh,time_offset,ci.audit_timestamp) &lt; cast(ltrim(rtrim(cast(year(getdate()) as char))) +ltrim(rtrim('-'))+
																ltrim(rtrim(cast(month(getdate()) as char)))+ltrim(rtrim('-'))+ltrim(rtrim('01'))
																as datetime)
																and    c.CERTIFICATE_IDENTIFIER =ci.CERTIFICATE_IDENTIFIER
																group by ci.CERTIFICATE_IDENTIFIER)

	UNION

	select
		--concat(CAST(CERTIFICATE_IDENTIFIER AS VARCHAR(12)), '*', CAST(NULL AS VARCHAR(12)), '*', CAST(NULL AS VARCHAR(12))) as compound_key, 
		c.CLAIM_ID,
			CONTRACT_IDENTIFIER,
			INSURED_LOAN_IDENTIFIER,
			NULL as CERTIFICATE_IDENTIFIER,
			cv1.SHORT_NAME as STATUS_CODE,
			cv2.SHORT_NAME as TYPE_CODE,
			cv3.SHORT_NAME as STATUS_CHANGE_REASON_CODE,
			ISNULL(C.LINE_OF_BUSINESS,0) AS LINE_OF_BUSINESS -- Modified 20170118; BI-1610
	from   dbo.STG_CLAIM c
			left outer join dbo.STG_CLAIM_CODE_VALUE cv1
				on c.STATUS_CODE = cv1.CODE_VALUE_ID
			left outer join dbo.STG_CLAIM_CODE_VALUE cv2
				on c.TYPE_CODE = cv2.CODE_VALUE_ID
			left outer join dbo.STG_CLAIM_CODE_VALUE cv3
				on STATUS_CHANGE_REASON_CODE = cv3.CODE_VALUE_ID,
				edw_dm.dbo.LKP_TIME_ZONE_CONVERSION LKP
	where  CONTRACT_IDENTIFIER is not NULL
	and    INSURED_LOAN_IDENTIFIER is not NULL
	and    cv2.SHORT_NAME  ='CLMI'
	and    c.audit_timestamp &gt;=time_offset_start_date
	and    c.audit_timestamp &lt; time_offset_end_date
	and    dateadd(hh,time_offset,c.audit_timestamp) = ( select max(dateadd(hh,time_offset,ci.audit_timestamp))
															from   dbo.STG_CLAIM ci,
																		dbo.STG_CLAIM_CODE_VALUE cvi,
																		edw_dm.dbo.LKP_TIME_ZONE_CONVERSION LKPi
																where  ci.CONTRACT_IDENTIFIER is not NULL
																and       ci.INSURED_LOAN_IDENTIFIER is not NULL
																and    ci.TYPE_CODE = cvi.CODE_VALUE_ID
																and    cvi.SHORT_NAME  ='CLMI'
																and    ci.audit_timestamp &gt;=LKPi.time_offset_start_date
																and    ci.audit_timestamp &lt; LKPi.time_offset_end_date
																and       dateadd(hh,time_offset,ci.audit_timestamp) &lt; cast(ltrim(rtrim(cast(year(getdate()) as char))) +ltrim(rtrim('-'))+
																ltrim(rtrim(cast(month(getdate()) as char)))+ltrim(rtrim('-'))+ltrim(rtrim('01'))
																as datetime)
																and    c.CONTRACT_IDENTIFIER =ci.CONTRACT_IDENTIFIER
																and       c.INSURED_LOAN_IDENTIFIER =ci.INSURED_LOAN_IDENTIFIER
																group by ci.INSURED_LOAN_IDENTIFIER,ci.CONTRACT_IDENTIFIER)
) a;</property>
