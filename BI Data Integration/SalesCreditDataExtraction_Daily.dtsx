                  <properties>
                    <property
                      dataType="System.Int32"
                      description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                      name="CommandTimeout">0</property>
                    <property
                      dataType="System.String"
                      description="Specifies the name of the database object used to open a rowset."
                      name="OpenRowset"></property>
                    <property
                      dataType="System.String"
                      description="Specifies the variable that contains the name of the database object used to open a rowset."
                      name="OpenRowsetVariable"></property>
                    <property
                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT 
CASE WHEN [Business_Unit_ID] IN ('00217','00260','00207','00250') THEN Certificate_Number END AS CERTIFICATE_NBR_BK_AMIWEST,
CASE WHEN [Business_Unit_ID] IN ('00522','00525') THEN Certificate_Number END AS CERTIFICATE_NBR_BK_UG,
CASE 
WHEN [Business_Unit_ID] IN ('00217','00260','00207','00250') THEN LEFT(cast(Certificate_Number as varchar(10)) + '*' + isnull(cast([Business_Unit_ID] as varchar),'') + '*' + cast(FDV_W.Insurance_Line_Code as varchar) + '*' + FDV_W.Loan_Line_Code, 20) 
WHEN [Business_Unit_ID] IN ('00522','00525') THEN LEFT(cast(Certificate_Number as varchar(10)) + '*' + isnull(cast([Business_Unit_ID] as varchar),'') + '*' + cast(FDV_E.Insurance_Line_Code as varchar) + '*' + FDV_E.Loan_Line_Code, 20)
END AS Certificate_Number,
Specified_Credit AS Specified_Credit,
Applied_Credit AS Applied_Credit,
Credit_Level AS Credit_Level,
Credit_Percent AS Credit_Percent,
OUPB AS OUPB,
Credited_Amount AS Credited_Amount,
Credited_Rep AS Credited_Rep,
Certificate_Status AS Certificate_Status,
AsOf AS AsOf,
Credit_Status AS Credit_Status,
Credited_Branch AS Credited_Branch,
Sales_Rep_Email AS Sales_Rep_Email,
Business_Unit_ID AS Business_Unit_ID
FROM DBO.STG_FCT_SALES_CREDIT FSC 
LEFT JOIN EDW_DM.DBO.FCT_DAILY_VOLUME FDV_E  
ON FSC.CERTIFICATE_NUMBER = FDV_E.CERTIFICATE_NBR_BK_UG
AND FDV_E.CERTIFICATE_NBR_BK_UG IS NOT NULL
LEFT JOIN EDW_DM.DBO.FCT_DAILY_VOLUME FDV_W 
ON FSC.CERTIFICATE_NUMBER = FDV_W.CERTIFICATE_NBR_BK_AMIWEST
AND FDV_W.CERTIFICATE_NBR_BK_AMIWEST IS NOT NULL;</property>
