                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT
	CHILD,
	PARENT,
	LINE1,
	LINE2,
	LAST_LINE
from  (
	SELECT
		DISTINCT 
		1 as "SortCol",
		'AddUserClass, "' + DEPT_LVL1_CD + '",' 	AS "CHILD" ,
		'"Cost Centers" /n'		AS "PARENT",
		'SetUserClassProperty,"'+ DEPT_LVL1_CD+ '" ,"UserClassPrivileges", "NONE" /n' AS "LINE1",
		'SetUserClassProperty,"'+ DEPT_LVL1_CD+ '" ,"Description", "' + DEPT_LVL1_NM + '" /n' AS "LINE2",
		'SetUserClassProperty,"'+ DEPT_LVL1_CD+ '" ,"UserPrivileges", "NONE" /n' AS "LAST_LINE"
	FROM dbo.DEPARTMENT
	WHERE 
		DEPT_LVL1_CD &lt;&gt; '«Unknown»'
	UNION

	SELECT
		DISTINCT 
		2 as "SortCol",
		'AddUserClass, "' + DEPT_LVL2_CD + '",' 	AS "CHILD" ,
		'"'+DEPT_LVL1_CD+'" /n'			AS "PARENT",
		'SetUserClassProperty,"'+ DEPT_LVL2_CD+ '" ,"UserClassPrivileges", "NONE" /n' AS "LINE1",
		'SetUserClassProperty,"'+ DEPT_LVL2_CD+ '" ,"Description", "' + DEPT_LVL2_NM + '" /n' AS "LINE2",
		'SetUserClassProperty,"'+ DEPT_LVL2_CD+ '" ,"UserPrivileges", "NONE" /n' AS "LAST_LINE"
	FROM dbo.DEPARTMENT
	WHERE 
		DEPT_LVL1_CD &lt;&gt; '«Unknown»' AND
		DEPT_LVL2_CD &lt;&gt; '«Unknown»' 

	UNION

	SELECT
		DISTINCT 
		3 as "SortCol",
		'AddUserClass, "' + DEPT_LVL3_CD + '",' 	AS "CHILD" ,
		'"'+DEPT_LVL2_CD+'" /n'			AS "PARENT",
		'SetUserClassProperty,"'+ DEPT_LVL3_CD+ '" ,"UserClassPrivileges", "NONE" /n' AS "LINE1",
		'SetUserClassProperty,"'+ DEPT_LVL3_CD+ '" ,"Description", "' + DEPT_LVL3_NM + '" /n' AS "LINE2",		
		'SetUserClassProperty,"'+ DEPT_LVL3_CD+ '" ,"UserPrivileges", "NONE" /n' AS "LAST_LINE"
	FROM dbo.DEPARTMENT
	WHERE 
		DEPT_LVL2_CD &lt;&gt; '«Unknown»' AND
		DEPT_LVL3_CD &lt;&gt; '«Unknown»' 

	UNION

	SELECT
		DISTINCT 
		4 as "SortCol",
		'AddUserClass, "' + DEPT_LVL4_CD + '",' 	AS "CHILD" ,
		'"'+DEPT_LVL3_CD+'" /n'			AS "PARENT",
		'SetUserClassProperty,"'+ DEPT_LVL4_CD+ '" ,"UserClassPrivileges", "NONE" /n' AS "LINE1",
		'SetUserClassProperty,"'+ DEPT_LVL4_CD+ '" ,"Description", "' + DEPT_LVL4_NM + '" /n' AS "LINE2",
		'SetUserClassProperty,"'+ DEPT_LVL4_CD+ '" ,"UserPrivileges", "NONE" /n' AS "LAST_LINE"
	FROM dbo.DEPARTMENT
	WHERE 
		DEPT_LVL3_CD &lt;&gt; '«Unknown»' AND
		DEPT_LVL4_CD &lt;&gt; '«Unknown»' 
	UNION

	SELECT
		DISTINCT 
		5 as "SortCol",
		'AddUserClass, "' + DEPT_LVL5_CD + '",' 	AS "CHILD" ,
		'"'+DEPT_LVL4_CD+'" /n'			AS "PARENT",
		'SetUserClassProperty,"'+ DEPT_LVL5_CD+ '" ,"UserClassPrivileges", "NONE" /n' AS "LINE1",
		'SetUserClassProperty,"'+ DEPT_LVL5_CD+ '" ,"Description", "' + DEPT_LVL5_NM + '" /n' AS "LINE2",
		'SetUserClassProperty,"'+ DEPT_LVL5_CD+ '" ,"UserPrivileges", "NONE" /n' AS "LAST_LINE"
	FROM dbo.DEPARTMENT
	WHERE 
		DEPT_LVL4_CD &lt;&gt; '«Unknown»' AND
		DEPT_LVL5_CD &lt;&gt; '«Unknown»' 
	UNION

	SELECT
		DISTINCT 
		6 as "SortCol",
		'AddUserClass, "' + DEPT_LVL6_CD + '",' 	AS "CHILD" ,
		'"'+DEPT_LVL5_CD+'" /n'			AS "PARENT",
		'SetUserClassProperty,"'+ DEPT_LVL6_CD+ '" ,"UserClassPrivileges", "NONE" /n' AS "LINE1",
		'SetUserClassProperty,"'+ DEPT_LVL6_CD+ '" ,"Description", "' + DEPT_LVL6_NM + '" /n' AS "LINE2",
		'SetUserClassProperty,"'+ DEPT_LVL6_CD+ '" ,"UserPrivileges", "NONE" /n' AS "LAST_LINE"
	FROM dbo.DEPARTMENT
	WHERE 
		DEPT_LVL5_CD &lt;&gt; '«Unknown»' AND
		DEPT_LVL6_CD &lt;&gt; '«Unknown»' 
	UNION

	SELECT
		DISTINCT 
		7 as "SortCol",
		'AddUserClass, "' + DEPT_LVL7_CD + '",' 	AS "CHILD" ,
		'"'+DEPT_LVL6_CD+'" /n'			AS "PARENT",
		'SetUserClassProperty,"'+ DEPT_LVL7_CD+ '" ,"UserClassPrivileges", "NONE" /n' AS "LINE1",
		'SetUserClassProperty,"'+ DEPT_LVL7_CD+ '" ,"Description", "' + DEPT_LVL7_NM + '" /n' AS "LINE2",
		'SetUserClassProperty,"'+ DEPT_LVL7_CD+ '" ,"UserPrivileges", "NONE" /n' AS "LAST_LINE"
	FROM dbo.DEPARTMENT
	WHERE 
		DEPT_LVL6_CD &lt;&gt; '«Unknown»' AND
		DEPT_LVL7_CD &lt;&gt; '«Unknown»' 
	UNION

	SELECT
		DISTINCT 
		8 as "SortCol",
		'AddUserClass, "' + DEPT_LVL8_CD + '",' 	AS "CHILD" ,
		'"'+DEPT_LVL7_CD+'" /n'			AS "PARENT",
		'SetUserClassProperty,"'+ DEPT_LVL8_CD+ '" ,"UserClassPrivileges", "NONE" /n' AS "LINE1",
		'SetUserClassProperty,"'+ DEPT_LVL8_CD+ '" ,"Description", "' + DEPT_LVL8_NM + '" /n' AS "LINE2",
		'SetUserClassProperty,"'+ DEPT_LVL8_CD+ '" ,"UserPrivileges", "NONE" /n' AS "LAST_LINE"
	FROM dbo.DEPARTMENT
	WHERE 
		DEPT_LVL7_CD &lt;&gt; '«Unknown»' AND
		DEPT_LVL8_CD &lt;&gt; '«Unknown»' 
	UNION

	SELECT
		DISTINCT 
		9 as "SortCol",
		'AddUserClass, "' + DEPARTMENT_CD + '",' 	AS "CHILD" ,
		Case 
			When DEPT_LVL7_CD &lt;&gt; '«Unknown»' then '"'+DEPT_LVL7_CD+'" /n'
			When DEPT_LVL6_CD &lt;&gt; '«Unknown»' then '"'+DEPT_LVL6_CD+'" /n'
			When DEPT_LVL5_CD &lt;&gt; '«Unknown»' then '"'+DEPT_LVL5_CD+'" /n'
			When DEPT_LVL4_CD &lt;&gt; '«Unknown»' then '"'+DEPT_LVL4_CD+'" /n'
			When DEPT_LVL3_CD &lt;&gt; '«Unknown»' then '"'+DEPT_LVL3_CD+'" /n'
			When DEPT_LVL2_CD &lt;&gt; '«Unknown»' then '"'+DEPT_LVL2_CD+'" /n'
			When DEPT_LVL1_CD &lt;&gt; '«Unknown»' then '"'+DEPT_LVL1_CD+'" /n'
		End AS "PARENT",
		'SetUserClassProperty,"'+ DEPARTMENT_CD+ '" ,"UserClassPrivileges", "NONE" /n' AS "LINE1",
		'SetUserClassProperty,"'+ DEPARTMENT_CD+ '" ,"Description", "' + DEPARTMENT_NM + '" /n' AS "LINE2",
		'SetUserClassProperty,"'+ DEPARTMENT_CD+ '" ,"UserPrivileges", "NONE" /n' AS "LAST_LINE"
	FROM dbo.DEPARTMENT
	WHERE 
