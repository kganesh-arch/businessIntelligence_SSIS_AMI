        <SQLTask:SqlTaskData
          SQLTask:Connection="{6198266F-E4D0-4DE1-A995-F4CBD87D6D1D}"
          SQLTask:SqlStatementSource="delete dbo.STG_OPUS_CERTIFICATE&#xA;from dbo.STG_OPUS_CERTIFICATE C &#xA;join dbo.STG_OPUS_LOAN L on L.Cert_ID = C.Cert_ID &#xA;join dbo.STG_OPUS_BRANCH OB on L.Loan_Orig_Branch_ID = OB.Branch_ID &#xA;join dbo.STG_OPUS_CUSTOMER cust on cust.Cust_ID = OB.Branch_Cust_ID &#xA;join dbo.HLP_TEST_LENDER tl on cust.Cust_Number = tl.LenderID;&#xA;&#xA;Delete dbo.stg_opus_certificate &#xA;from dbo.stg_opus_certificate opcert&#xA;join dbo.STG_OPUS_LOAN_BORROWER Bor&#xA; on opcert.Cert_ID = bor.Cert_ID&#xA; and bor.Loan_Brwr_Name_First = 'Test' &#xA;       and  bor.Loan_Brwr_Name_Last = 'Test';&#xA;&#xA;&#xA;&#xA;delete dbo.stg_opus_rate_quote &#xA;from dbo.stg_opus_rate_quote rq&#xA;join dbo.HLP_TEST_LENDER h&#xA; on 1=1&#xA; and h.LenderID not in (21000,&#xA;       13296,&#xA;       15758,&#xA;       15883,&#xA;       16380,&#xA;       16381,&#xA;       16382,&#xA;       16383&#xA;      )&#xA; and cast(left(rq.MPN,case when charindex('-',  rq.MPN, 0)-1 &gt; 0 then charindex('-',  rq.MPN, 0)-1 else 0 end) as int) = h.LenderID&#xA; and isnull(rq.MPN,'') &lt;&gt; ''&#xA;where cast(left(rq.MPN,case when charindex('-',  rq.MPN, 0)-1 &gt; 0 then charindex('-',  rq.MPN, 0)-1 else 0 end) as int) &#xA;not in (21000,&#xA;       13296,&#xA;       15758,&#xA;       15883,&#xA;       16380,&#xA;       16381,&#xA;       16382,&#xA;       16383&#xA;      );&#xA;&#xA;&#xA;delete dbo.STG_OPUS_CERTIFICATE &#xA;from dbo.STG_OPUS_CERTIFICATE opcert&#xA;join dbo.stg_opus_loan ln&#xA; on opcert.cert_id = ln.Cert_ID&#xA; and (ln.Loan_Number like '%test%')&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
