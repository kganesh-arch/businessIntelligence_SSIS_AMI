      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{6198266F-E4D0-4DE1-A995-F4CBD87D6D1D}"
          SQLTask:SqlStatementSource="-- table for reconstructing decision history&#xA;declare @Decision_History as Table &#xA;       (&#xA;                     Decision_ID int&#xA;              ,      Decision_Item_Type_Code int&#xA;              ,      Decision_Item_ID int&#xA;              ,      Decision_Emp_Number char(11)&#xA;              ,      Decision_Date datetime&#xA;              ,      Decision_Type_Code int&#xA;              ,      Decision_SubType_Code int&#xA;              --,    [timestamp] timestamp&#xA;             ,      Decision_Emp_Modified varchar(6)&#xA;              ,      Decision_App_Type_Code int&#xA;       )&#xA;&#xA;-- first, add entries from the Decision table&#xA;insert @Decision_History (               &#xA;                     Decision_ID &#xA;              ,      Decision_Item_Type_Code &#xA;              ,      Decision_Item_ID &#xA;              ,      Decision_Emp_Number &#xA;              ,      Decision_Date &#xA;              ,      Decision_Type_Code &#xA;              ,      Decision_SubType_Code &#xA;              ,      Decision_Emp_Modified &#xA;              ,      Decision_App_Type_Code &#xA;       )&#xA;select               D.Decision_ID &#xA;              ,      D.Decision_Item_Type_Code &#xA;              ,      D.Decision_Item_ID &#xA;              ,      D.Decision_Emp_Number &#xA;              ,      D.Decision_Date &#xA;              ,      D.Decision_Type_Code &#xA;              ,      D.Decision_SubType_Code &#xA;              ,      D.Decision_Emp_Modified &#xA;              ,      D.Decision_App_Type_Code &#xA;  from edw_staging.dbo.stg_opus_certificate as C&#xA;       join edw_staging.dbo.stg_opus_decision_as_is_from_opus as D on D.Decision_Item_ID = C.Cert_ID and D.Decision_Item_Type_Code = 262    &#xA;          and c.cert_date_created &gt; = dateadd(dd,-1000,getdate()) -- '2015-01-01' &#xA;&#xA;           &#xA;-- reconstruct Decision By changes&#xA;-- Decision.Decision_Date is the original decision date, Decision.Decision_Emp_Number is the latest Decision By&#xA;-- Change_History.ChgHist_OldValue has the original Decision By value&#xA;-- Change_History.ChgHist_Datetime has when the Decision By was changed&#xA;&#xA;&#xA;update @Decision_History&#xA;   set Decision_Emp_Number = &#xA;    (select top 1 [Emp_Number]&#xA;  from edw_staging.dbo.stg_opus_employee &#xA;   where LTRIM(RTRIM(isnull(Emp_Name_First,'') + ' ' + isnull(Emp_Name_Last,''))) =&#xA;     (select top 1 LTRIM(RTRIM(ChgHist_OldValue)) from edw_staging.dbo.stg_opus_Change_History &#xA;      where ChgHist_Object_ID = [@Decision_History].Decision_Item_ID &#xA;      and ChgHist_TagID2 = [@Decision_History].Decision_ID and UI_Field_Name = 'Program Decision Made By' order by ChgHist_DateTime asc)&#xA;       )      &#xA;       where exists (select ChgHist_Object_ID from edw_staging.dbo.stg_opus_Change_History where ChgHist_Object_ID = [@Decision_History].Decision_Item_ID and ChgHist_TagID2 = [@Decision_History].Decision_ID and UI_Field_Name = 'Program Decision Made By')&#xA;       &#xA;&#xA;-- THIS STEP MOVED UP&#xA;-- handle Decision Date changes&#xA;update @Decision_History&#xA;   set Decision_Date = (select top 1 ChgHist_OldValue from edw_staging.dbo.stg_opus_Change_History where ChgHist_Object_ID = [@Decision_History].Decision_Item_ID and ChgHist_TagID2 = [@Decision_History].Decision_ID and UI_Field_Name = 'Program Decision Date' order by ChgHist_DateTime asc)&#xA;where exists (select ChgHist_Object_ID from edw_staging.dbo.stg_opus_Change_History where ChgHist_Object_ID = [@Decision_History].Decision_Item_ID and ChgHist_TagID2 = [@Decision_History].Decision_ID and UI_Field_Name = 'Program Decision Date')&#xA;&#xA;&#xA;insert @Decision_History (               &#xA;                     Decision_ID &#xA;              ,      Decision_Item_Type_Code &#xA;              ,      Decision_Item_ID &#xA;              ,      Decision_Emp_Number &#xA;              ,      D.Decision_Date &#xA;              ,      Decision_Type_Code &#xA;              ,      Decision_SubType_Code &#xA;              ,      Decision_Emp_Modified &#xA;              ,      Decision_App_Type_Code &#xA;       )  &#xA;select      &#xA;                                   D.Decision_ID &#xA;              ,      D.Decision_Item_Type_Code &#xA;              ,      D.Decision_Item_ID &#xA;              ,      D.Decision_Emp_Number &#xA;              ,      B.ChgHist_DateTime --D.Decision_Date &#xA;              ,      D.Decision_Type_Code &#xA;              ,      D.Decision_SubType_Code &#xA;              ,      D.Decision_Emp_Modified &#xA;              ,      D.Decision_App_Type_Code &#xA;  from &#xA;                 edw_staging.dbo.stg_opus_certificate C&#xA;       join edw_staging.dbo.stg_opus_decision_as_is_from_opus as D on D.Decision_Item_ID = C.Cert_ID and D.Decision_Item_Type_Code = 262    &#xA;       join edw_staging.dbo.stg_opus_Change_History as B on  B.ChgHist_Object_ID = D.Decision_Item_ID and B.ChgHist_TagID2 = D.Decision_ID and B.UI_Field_Name = 'Program Decision Made By'&#xA;&#xA;&#xA;-- Drop indexes to speed up the insert&#xA;IF  EXISTS (SELECT * FROM sys.indexes WHERE object_id = OBJECT_ID(N'[dbo].[stg_opus_decision]') AND name = N'idx_OPUS_DECISION_DecisionItemTypeCode')&#xA;DROP INDEX [idx_OPUS_DECISION_DecisionItemTypeCode] ON [dbo].[STG_OPUS_DECISION] WITH ( ONLINE = OFF )&#xA;&#xA;IF  EXISTS (SELECT * FROM sys.indexes WHERE object_id = OBJECT_ID(N'[dbo].[stg_opus_decision]') AND name = N'idx_OPUS_DECISION_DecisionItemID')&#xA;DROP INDEX [idx_OPUS_DECISION_DecisionItemID] ON [dbo].[STG_OPUS_DECISION] WITH ( ONLINE = OFF )&#xA;&#xA;IF  EXISTS (SELECT * FROM sys.indexes WHERE object_id = OBJECT_ID(N'[dbo].[stg_opus_decision]') AND name = N'idx_OPUS_DECISION_DecisionID')&#xA;DROP INDEX [idx_OPUS_DECISION_DecisionID] ON [dbo].[STG_OPUS_DECISION] WITH ( ONLINE = OFF )&#xA;&#xA;IF  EXISTS (SELECT * FROM sys.indexes WHERE object_id = OBJECT_ID(N'[dbo].[stg_opus_decision]') AND name = N'idx_OPUS_DECISION_DecisionEmpNumber')&#xA;DROP INDEX [idx_OPUS_DECISION_DecisionEmpNumber] ON [dbo].[STG_OPUS_DECISION] WITH ( ONLINE = OFF )&#xA;&#xA;IF  EXISTS (SELECT * FROM sys.indexes WHERE object_id = OBJECT_ID(N'[dbo].[stg_opus_decision]') AND name = N'idx_OPUS_DECISION_DecisionDate')&#xA;DROP INDEX [idx_OPUS_DECISION_DecisionDate] ON [dbo].[STG_OPUS_DECISION] WITH ( ONLINE = OFF )&#xA;&#xA;&#xA;truncate table edw_staging.dbo.stg_opus_decision&#xA;insert edw_staging.dbo.stg_opus_decision (               &#xA;                     Decision_ID &#xA;              ,      Decision_Item_Type_Code &#xA;              ,      Decision_Item_ID &#xA;              ,      Decision_Emp_Number &#xA;              ,      D.Decision_Date &#xA;              ,      Decision_Type_Code &#xA;              ,      Decision_SubType_Code &#xA;              ,      Decision_Emp_Modified &#xA;              ,      Decision_App_Type_Code &#xA;       )  &#xA;select      &#xA;                     D.Decision_ID &#xA;              ,      D.Decision_Item_Type_Code &#xA;              ,      D.Decision_Item_ID &#xA;              ,      D.Decision_Emp_Number &#xA;              ,      D.Decision_Date &#xA;              ,      D.Decision_Type_Code &#xA;              ,      D.Decision_SubType_Code &#xA;              ,      D.Decision_Emp_Modified &#xA;              ,      D.Decision_App_Type_Code &#xA;  from @Decision_History D&#xA;&#xA;&#xA;-- Recreate the indexes and stats to speed up reads&#xA;/****** object:  index [idx_opus_decision_decisiondate]    script date: 4/25/2014 11:37:42 am ******/&#xA;create nonclustered index [idx_opus_decision_decisiondate] on [dbo].[STG_OPUS_DECISION]&#xA;(&#xA; [decision_date] asc&#xA;)with (pad_index = off, statistics_norecompute = off, sort_in_tempdb = off, drop_existing = off, online = off, allow_row_locks = on, allow_page_locks = on) on [primary]&#xA;&#xA;set ansi_padding on&#xA;&#xA;&#xA;/****** object:  index [idx_opus_decision_decisionempnumber]    script date: 4/25/2014 11:37:42 am ******/&#xA;create nonclustered index [idx_opus_decision_decisionempnumber] on [dbo].[STG_OPUS_DECISION]&#xA;(&#xA; [decision_emp_number] asc&#xA;)with (pad_index = off, statistics_norecompute = off, sort_in_tempdb = off, drop_existing = off, online = off, allow_row_locks = on, allow_page_locks = on) on [primary]&#xA;&#xA;/****** object:  index [idx_opus_decision_decisionid]    script date: 4/25/2014 11:37:42 am ******/&#xA;create nonclustered index [idx_opus_decision_decisionid] on [dbo].[STG_OPUS_DECISION]&#xA;(&#xA; [decision_id] asc&#xA;)with (pad_index = off, statistics_norecompute = off, sort_in_tempdb = off, drop_existing = off, online = off, allow_row_locks = on, allow_page_locks = on) on [primary]&#xA;&#xA;/****** object:  index [idx_opus_decision_decisionitemid]    script date: 4/25/2014 11:37:42 am ******/&#xA;create nonclustered index [idx_opus_decision_decisionitemid] on [dbo].[STG_OPUS_DECISION]&#xA;(&#xA; [decision_item_id] asc&#xA;)with (pad_index = off, statistics_norecompute = off, sort_in_tempdb = off, drop_existing = off, online = off, allow_row_locks = on, allow_page_locks = on) on [primary]&#xA;&#xA;/****** object:  index [idx_opus_decision_decisionitemtypecode]    script date: 4/25/2014 11:37:42 am ******/&#xA;create nonclustered index [idx_opus_decision_decisionitemtypecode] on [dbo].[STG_OPUS_DECISION]&#xA;(&#xA; [decision_item_type_code] asc&#xA;)with (pad_index = off, statistics_norecompute = off, sort_in_tempdb = off, drop_existing = off, online = off, allow_row_locks = on, allow_page_locks = on) on [primary]&#xA;&#xA;set ansi_padding on&#xA;&#xA;&#xA;&#xA;update statistics edw_staging.dbo.stg_opus_decision with fullscan, norecompute &#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
