      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{6198266F-E4D0-4DE1-A995-F4CBD87D6D1D}"
          SQLTask:SqlStatementSource="&#xA;insert edw_staging.dbo.hlp_combined_opus_comments&#xA;select * &#xA;--into edw_staging.dbo.hlp_combined_opus_comments&#xA;from&#xA;(&#xA;select  &#xA;              MC.Entity_Id&#xA;       ,      MC.Entity_Type_Flag&#xA;       ,      MC.Comment_Id&#xA;       ,   MC.Comment_Emp_Number&#xA;    , emp.Emp_Name_First Employee_First_Name&#xA;    , emp.Emp_Name_Last Employee_Last_Name&#xA;       ,      MC.Comment_Date_Created&#xA;       ,      MC.Comment_Date_Modified&#xA;       ,   MC.comment_subject_code&#xA;    , oc.opus_code_string as [Comment Subject]&#xA;       ,   LTRIM((select ' ' + RTRIM(MCT.comment_text)&#xA;        from [dbo].[stg_opus_Comment_Text] MCT  &#xA;              where MC.Comment_Id = MCT.Comment_Id&#xA;              order by MCT.Comment_Id, MCT.comment_seq_id&#xA;           for xml path('') )) AS [Combined Comment Text]&#xA;&#xA;&#xA;&#xA;       from [dbo].[STG_OPUS_COMMENT] MC  &#xA;    left outer join [dbo].[stg_opus_Employee] emp&#xA;  on mc.Comment_Emp_Number = emp.emp_number&#xA;    left outer join [dbo].[STG_OPUS_CODE] oc&#xA;  on mc.comment_subject_code = oc.opus_code and oc.opus_code_type = 'LEtype'&#xA;&#xA;&#xA;       where&#xA;                     1=1 &#xA;              --and  Entity_id = 1 &#xA;              --and Comment_Date_Created &gt;= '01-01-2013' &#xA;              and Comment_Date_Created &gt; (select isnull(max(Comment_Date_Created),'01-01-2013') from edw_staging.dbo.hlp_combined_opus_comments)&#xA;&#xA;&#xA;) A&#xA;/*&#xA;where &#xA;       -- Delinquent &amp; Performing Loan Modification Comments&#xA;       ( [Combined Comment Text] like ('%Modification Approved%') &#xA;   or [Combined Comment Text] like ('%Modification Declined%') &#xA;   or [Combined Comment Text] like ('%Modification In Progress%')&#xA;  )&#xA;       -- Instant Loan Modification Comments&#xA;       OR [Combined Comment Text] like ('%Instant Loan Modification%')&#xA;       -- USS Comments for Registration Completed&#xA;       OR [Combined Comment Text] like ('%Registration Completed%')&#xA;       -- Expired Commitment Comments&#xA;       OR ([Combined Comment Text] like ('%Expired Commitment Approved%') &#xA;   or [Combined Comment Text] like ('%Expired Commitment Declined%'))&#xA;       -- Exception Comments &#xA;       OR ( comment_subject_code in (select oc.opus_code from [OPUS_REPORTING].[P_OpusDisc_CMG_US_01].[dbo].[opus_code] oc &#xA;         where oc.opus_code_type = 'LEtype'))&#xA;  OR [Combined Comment Text]  like ('%Assumption%')&#xA;  OR [Combined Comment Text] like ('%Independent Validation%') &#xA;  OR [Combined Comment Text] like ('%Requirements%') &#xA;*/" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
