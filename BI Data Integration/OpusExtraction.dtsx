          SQLTask:SqlStatementSource="if exists (select * from dbo.sysobjects where id = object_id(N'LKP_USS_LOAN_ORIGINATOR') and OBJECTPROPERTY(id, N'IsUserTable') = 1)&#xA;drop table dbo.LKP_USS_LOAN_ORIGINATOR&#xA;&#xA;select Certificate_Nbr_BK as CertID&#xA;  , max(Loan_Originator_Name) as [Loan Originator Name]&#xA;  , Max(LO_Company_Name) as [Loan Originator Company]&#xA;  ,max(LO_EMAIL_Address) as [Loan Originator Email]&#xA;  , Replace&#xA;   (Max(Case &#xA;    When LO_Phone_Number Is Null Then ''&#xA;    When Left(LO_Phone_Number,1) NOT between '0' and '9' Then LO_Phone_Number&#xA;    When Left(LO_Phone_Number,1) &lt;&gt; '(' Then '(' + Left(LO_Phone_Number,3) + ') ' + Right(LO_Phone_Number, Abs(Len(LO_Phone_Number)-3))&#xA;    Else LO_Phone_Number End&#xA;      ) &#xA;   , ') -'&#xA;   , ') '&#xA;   )as [Loan Originator Phone]&#xA; INTO dbo.LKP_USS_LOAN_ORIGINATOR&#xA; --from EDW_STAGING.dbo.STG_USS_Daily_Production_Log&#xA; from&#xA; (&#xA;&#x9;&#x9;SELECT&#x9;&#xA;&#x9;&#x9;&#x9;&quot;Cert_Number&quot; as Certificate_Nbr_BK,&#xA;&#x9;&#x9;&#x9;&quot;Loan_Ofcr_Name&quot; as Loan_Originator_Name,&#xA;&#x9;&#x9;&#x9;&quot;Loan_Ofcr_Phone&quot; as LO_Phone_Number,&#xA;&#x9;&#x9;&#x9;&quot;Loan_Ofcr_Email&quot; as LO_EMAIL_Address,&#xA;&#x9;&#x9;&#x9;&quot;Loan_Ofcr_Company&quot; as LO_Company_Name&#xA;&#x9;&#x9;FROM  EDW_STAGING.&quot;dbo&quot;.&quot;STG_OPUS_CERTIFICATE&quot;&#xA;&#x9;&#x9;where &#xA;&#x9;&#x9;&#x9;Cert_Date_Created &gt;= '06-18-2016'&#xA;&#x9;&#x9;&#x9;and&#xA;&#x9;&#x9;&#x9;(&#xA;&#x9;&#x9;&#x9;isnull(&quot;Loan_Ofcr_Name&quot;,'') &lt;&gt; ''&#xA;&#x9;&#x9;&#x9;or isnull(&quot;Loan_Ofcr_Phone&quot;,'') &lt;&gt; ''&#xA;&#x9;&#x9;&#x9;or isnull(&quot;Loan_Ofcr_Email&quot;,'') &lt;&gt; ''&#xA;&#x9;&#x9;&#x9;or isnull(&quot;Loan_Ofcr_Company&quot;,'') &lt;&gt; ''&#xA;&#x9;) &#xA; ) a&#xA; --where Loan_Originator_Name &lt;&gt; '' OR LO_EMAIL_Address &lt;&gt; ''&#xA; group by Certificate_Nbr_BK" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/