                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">select  distinct CLAIM.line_of_business,
        rtrim("CLAIM"."CERTIFICATE_IDENTIFIER") as "MI Certificate Number"   
       , sev.GSE_LOAN_NUMBER as "FNMA Loan Number" 
       , perfect_dt as "Perfect Date"       
       , Cast(clog.LOG_TEXT as varchar) as "Change Reason"  
from  
	PMICNTRL_CLAIMS.severity_factor as sev 
      INNER JOIN 
        "PMICNTRL_CLAIMS"."CLAIM" as  "CLAIM"
              on sev.certificate_identifier = "CLAIM"."CERTIFICATE_IDENTIFIER"
		inner join 	(select CERTIFICATE_IDENTIFIER, claim_id , min(Cast(audit_timestamp as Date)) as perfect_dt
						from "PMICNTRL_CLAIMS"."CLAIM_AUDIT" claudt
						where status_code = '5054' 
						and audit_timestamp &gt; (select max(audit_timestamp) from "PMICNTRL_CLAIMS"."CLAIM_AUDIT" claudtin 
													where claudt.CERTIFICATE_IDENTIFIER = claudtin.CERTIFICATE_IDENTIFIER
													and claudt.claim_id = claudtin.claim_id
													and status_code = '5053')
      						group by CERTIFICATE_IDENTIFIER, claim_id						
    				) as clmad on clmad.CERTIFICATE_IDENTIFIER =  "CLAIM"."CERTIFICATE_IDENTIFIER"
    						and clmad.claim_id = CLAIM.claim_id  
    						and perfect_dt = (select Cast(current_date as Date) - 1 day as thedate from sysibm.sysdummy1)    						             
       inner join "PMICNTRL_CLAIMS"."CODE_VALUE" "code_value_claim_type" on "CLAIM"."TYPE_CODE" = "code_value_claim_type"."CODE_VALUE_ID"
       LEFT OUTER JOIN "PMICNTRL_PARTY"."PARTY" "PARTY" on "CLAIM"."FILER_PARTY_IDENTIFIER" = "PARTY"."PARTY_ID"
       LEFT OUTER JOIN 
              (SELECT CERTIFICATE_IDENTIFIER, LOG_TEXT, RNUM FROM 
                     (
                     SELECT CERTIFICATE_IDENTIFIER, LOG_TEXT , 
                     ROW_NUMBER() OVER (PARTITION BY CERTIFICATE_IDENTIFIER ORDER BY AUDIT_TIMESTAMP DESC) AS RNUM  
                     FROM PMICNTRL_CLAIMS.COMMUNICATION_LOG 
                     where 1=1
                     and SOURCE_TYPE_CODE = '7121'
                     )
                     WHERE RNUM = 1  
              ) as clog on clog.CERTIFICATE_IDENTIFIER = "CLAIM"."CERTIFICATE_IDENTIFIER"                 
 where 1=1 
	  AND "code_value_claim_type"."SHORT_NAME" = 'CLMI'
		and "CLAIM"."STATUS_CODE" &lt;&gt; '5053'  
		and  CLAIM.line_of_business = 1   
		
union

select  distinct CLAIM.line_of_business,
        rtrim("CLAIM"."CERTIFICATE_IDENTIFIER") as "MI Certificate Number"   
       , sev.GSE_LOAN_NUMBER as "FNMA Loan Number" 
       , Cast(CLAIM.audit_timestamp as Date) as "Perfect Date"        
       , Cast(clog.LOG_TEXT as varchar) as "Change Reason"  
from  
	PMICNTRL_CLAIMS.severity_factor as sev 
        INNER JOIN 
        "PMICNTRL_CLAIMS"."CLAIM" as "CLAIM"
              on sev.certificate_identifier = "CLAIM"."CERTIFICATE_IDENTIFIER"
       inner join (select CERTIFICATE_IDENTIFIER, STATUS_CODE --, AUDIT_TIMESTAMP 
						 from "PMICNTRL_CLAIMS"."CLAIM_AUDIT" claudt
						where AUDIT_TIMESTAMP = (select max(AUDIT_TIMESTAMP) from "PMICNTRL_CLAIMS"."CLAIM_AUDIT" claudtin
													where claudtin.CERTIFICATE_IDENTIFIER = claudt.CERTIFICATE_IDENTIFIER)
					) as clmad
				on clmad.CERTIFICATE_IDENTIFIER = "CLAIM"."CERTIFICATE_IDENTIFIER"
				and clmad.STATUS_CODE = '5053'
       inner join "PMICNTRL_CLAIMS"."CODE_VALUE" "code_value_claim_type" on "CLAIM"."TYPE_CODE" = "code_value_claim_type"."CODE_VALUE_ID"
       LEFT OUTER JOIN "PMICNTRL_PARTY"."PARTY" "PARTY" on "CLAIM"."FILER_PARTY_IDENTIFIER" = "PARTY"."PARTY_ID"
       LEFT OUTER JOIN 
              (SELECT CERTIFICATE_IDENTIFIER, LOG_TEXT, RNUM FROM 
                     (
                     SELECT CERTIFICATE_IDENTIFIER, LOG_TEXT , 
                     ROW_NUMBER() OVER (PARTITION BY CERTIFICATE_IDENTIFIER ORDER BY AUDIT_TIMESTAMP DESC) AS RNUM  
                     FROM PMICNTRL_CLAIMS.COMMUNICATION_LOG 
                     where 1=1
                     and SOURCE_TYPE_CODE = '7121'
                     )
                     WHERE RNUM = 1  
              ) as clog on clog.CERTIFICATE_IDENTIFIER = "CLAIM"."CERTIFICATE_IDENTIFIER"                 
 where 1=1 
	  AND "code_value_claim_type"."SHORT_NAME" = 'CLMI'
		and "CLAIM"."STATUS_CODE" = '5054'  
		and  CLAIM.line_of_business = 1
		and Cast(CLAIM.audit_timestamp as Date) = (select Cast(current_date as Date) - 1 day as thedate from sysibm.sysdummy1)</property>
