                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">select 
getdate() as Extract_date,
XML_Originating_Source, 
XMLTransactionID, 
documentid,
datetimelogged, 
CERTIFICATE_NBR,
CASE WHEN XML_Originating_Source = 'EDI' THEN XMLPARSE.Application_Type 
WHEN XML_Originating_Source = 'Arch MI Connect' THEN cast(op_FV.factor_value_name as varchar(50)) 
END AS Application_Type,
CASE WHEN XML_Originating_Source = 'EDI' THEN XMLPARSE.SUBMISSION_TYPE 
when XML_Originating_Source = 'Arch MI Connect' AND XMLPARSE.SUBMISSION_TYPE = 0 THEN 'OriginalRequest' 
when XML_Originating_Source = 'Arch MI Connect' AND XMLPARSE.SUBMISSION_TYPE = 1 THEN 'Resubmission' 
END  AS Submission_Type,
CASE WHEN XML_Originating_Source = 'EDI' THEN XMLPARSE.DECISION_TYPE ELSE isnull(cast(OP_Cd.OPUS_Code_String as varchar(30)),'Not Found') END AS Decision_Type
FROM
(
select 
CASE 
WHEN SoapActionName = 'http://tempuri.org/action/WebOrigination.GenerateCertificate' THEN 
'Arch MI Connect' 
WHEN SoapActionName = 'Weborigination.mismoorigination' THEN 
'EDI' 
END AS XML_Originating_Source,
op_xmltxd.XMLTransactionID AS XMLTransactionID,
op_xmltxd.DateTimeLogged AS datetimelogged,
op_fd.documentid,
CASE 
WHEN SoapActionName = 'http://tempuri.org/action/WebOrigination.GenerateCertificate' THEN 
ISNULL(Cast(Replace(Cast(TextData as varchar(max)), '&lt;?xml version="1.0" encoding="UTF-8" standalone="no"?&gt;', '&lt;?xml version="1.0" standalone="no"?&gt;') as xml).value('(//Service//Result/position_32/position_32_3_0)[1]', 'varchar(30)'), 'Not Found') 
WHEN SoapActionName = 'Weborigination.mismoorigination' THEN 
ISNULL(Cast(Replace(Cast(TextData as varchar(max)), '&lt;?xml version="1.0" encoding="UTF-8" standalone="no"?&gt;', '&lt;?xml version="1.0" standalone="no"?&gt;') as xml).value('(//MI_RESPONSE/@MICertificateIdentifier)[1]', 'varchar(30)'), 'Not Found')
END
as certificate_nbr,
CASE 
WHEN SoapActionName = 'http://tempuri.org/action/WebOrigination.GenerateCertificate' THEN 
ISNULL(Cast(Replace(Cast(TextData as varchar(max)), '&lt;?xml version="1.0" encoding="UTF-8" standalone="no"?&gt;', '&lt;?xml version="1.0" standalone="no"?&gt;') as xml).value('(//Service//Result/position_32/position_32_6_0)[1]', 'varchar(30)'), 'Not Found') 
WHEN SoapActionName = 'Weborigination.mismoorigination' THEN 
ISNULL(Cast(Replace(Cast(TextData as varchar(max)), '&lt;?xml version="1.0" encoding="UTF-8" standalone="no"?&gt;', '&lt;?xml version="1.0" standalone="no"?&gt;') as xml).value('(//MI_RESPONSE/@MIApplicationType)[1]', 'varchar(30)'), 'Not Found') 
END AS Application_Type,
CASE 
WHEN SoapActionName = 'http://tempuri.org/action/WebOrigination.GenerateCertificate' THEN 
ISNULL(Cast(Replace(Cast(TextData as varchar(max)), '&lt;?xml version="1.0" encoding="UTF-8" standalone="no"?&gt;', '&lt;?xml version="1.0" standalone="no"?&gt;') as xml).value('(//Service//Result/position_32/position_32_30_0)[1]', 'varchar(30)'), 'Not Found') 
WHEN SoapActionName = 'Weborigination.mismoorigination' THEN 
ISNULL(Cast(Replace(Cast(TextData as varchar(max)), '&lt;?xml version="1.0" encoding="UTF-8" standalone="no"?&gt;', '&lt;?xml version="1.0" standalone="no"?&gt;') as xml).value('(//PMI_CERTIFICATE_DATA/@MIRequestType)[1]', 'varchar(30)'), 'Not Found') 
END as Submission_Type,
CASE 
WHEN SoapActionName = 'http://tempuri.org/action/WebOrigination.GenerateCertificate' THEN 
ISNULL(Cast(Replace(Cast(TextData as varchar(max)), '&lt;?xml version="1.0" encoding="UTF-8" standalone="no"?&gt;', '&lt;?xml version="1.0" standalone="no"?&gt;') as xml).value('(//Service//Result/position_27/position_27_6_0)[1]', 'varchar(30)'), 'Not Found') 
WHEN SoapActionName = 'Weborigination.mismoorigination' THEN 
ISNULL(Cast(Replace(Cast(TextData as varchar(max)), '&lt;?xml version="1.0" encoding="UTF-8" standalone="no"?&gt;', '&lt;?xml version="1.0" standalone="no"?&gt;') as xml).value('(//MI_RESPONSE/@MIDecisionType)[1]', 'varchar(30)'), 'Not Found') 
END as Decision_Type
from dbo.FILEDOCUMENT op_fd with (nolock)
join dbo.xmltxdocument op_xmltxd  with (nolock)
on op_fd.documentid = op_xmltxd.documentid
join dbo.XMLTransaction op_xmltx  with (nolock)
on op_xmltxd.XMLTransactionID = op_xmltx.XMLTransactionID
where 
op_fd.TextData is not null and 
op_xmltxd.DateTimeLogged &gt;= (getdate() - 5)
and op_xmltxd.XMLTXDocumentType = 'Response'
and SoapActionName in 
('http://tempuri.org/action/WebOrigination.GenerateCertificate', 
'Weborigination.mismoorigination')
and try_convert(xml,Cast(Replace(Cast(TextData as varchar(max)), '&lt;?xml version="1.0" encoding="UTF-8" standalone="no"?&gt;', '&lt;?xml version="1.0" standalone="no"?&gt;') as xml)) is not null 
) AS XMLPARSE
left join FACTOR_VALUE op_fv with (nolock)
on 
XMLPARSE.Application_Type= cast(op_fv.factor_value_id  as varchar)
left join opus_code op_cd with (nolock)
on
XMLPARSE.Decision_Type= cast(op_cd.opus_code_id as varchar)
where xmlparse.certificate_nbr &lt;&gt; 'Not Found'

UNION 

