cast(JSON_VALUE(response_text,'$.MIRequestType') as varchar(30)) AS Submission_Type,
cast(JSON_VALUE(response_text,'$.MIDecisionType') as varchar(30)) AS Decision_Type
FROM DBO.Service_Call_Log where 
operation = 'SecureOriginationServices.OriginateCertificate' and 
Response_Text is not null
